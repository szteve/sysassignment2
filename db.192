;
; BIND reverse data file
;
$TTL    604800
@       IN      SOA     ns.example.lan. root.ns.example.lan. (
                              2         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      ns.
5       IN      PTR     ns.example.lan.
11	IN	PTR	www.example.lan.
